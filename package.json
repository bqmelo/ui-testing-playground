{
  "name": "ui-testing-playground",
  "version": "1.0.0",
  "description": "The purpose of this project is to test the UI Test Automation Playground (http://www.uitestingplayground.com/) using Cypress.",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:spec": "cypress run --spec",
    "cy:run:ci": "node_modules/.bin/cypress run",
    "cleanup:reports": "rm -R -f cypress/reports && rm -R -f report && mkdir report",
    "combine-reports": "mochawesome-merge --reportDir cypress/reports/mocha-reports/*.json > report/report.json",
    "generate-html": "marge report/*.json -f report -o report",
    "cy:run:report": "npm run cleanup:reports && npm run cy:run && npm run combine-reports && npm run generate-html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bqmelo/ui-testing-playground.git"
  },
  "author": "Bruna Melo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bqmelo/ui-testing-playground/issues"
  },
  "homepage": "https://github.com/bqmelo/ui-testing-playground#readme",
  "devDependencies": {
    "cypress": "^9.5.4",
    "cypress-xpath": "^1.6.2",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "start-server-and-test": "^1.14.0"
  }
}
